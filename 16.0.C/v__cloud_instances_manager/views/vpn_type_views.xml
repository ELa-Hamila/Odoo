<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="action_vpn_type" model="ir.actions.act_window">
            <field name="name">VPN type</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">vpn.type</field>
            <field name="view_mode">tree</field>
        </record>

        <record id="view_vpn_type_tree" model="ir.ui.view">
            <field name="name">vpn.type.tree</field>
            <field name="model">vpn.type</field>
            <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="name"/>
                </tree>
            </field>
        </record>
    </data>
</odoo>