<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="action_cloud_instances" model="ir.actions.act_window">
            <field name="name">Cloud instances </field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">cloud.instance</field>
            <field name="view_mode">tree,form</field>
        </record>
        <record id="odoo_version_tree" model="ir.ui.view">
            <field name="name">odoo.version.tree</field>
            <field name="model">odoo.version</field>
            <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="name"/>
                </tree>
            </field>
        </record>
    </data>
</odoo>